#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int a,b,c;
    scanf("%d",&a);
    printf("%d",a);
    for(int i=0;i<a;i++){
        scanf("%d",b);
        while(b>0){
            c=b%10;
            printf("%d",c);
            b=b/10;}
       printf("\n"); 
    }
                
    
    return 0;
}
