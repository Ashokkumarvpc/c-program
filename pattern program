12345


#include <stdio.h>
int main()
{
    int a;
    scanf("%d",&a);
    for(int i=0;i<a;i++)
    {
        int n=1;
        for(int j=0;j<a;j++)
        {
             
            if(j<=i)
                 printf("%d",n);
                 n++;
         
        }
        printf("\n");
    } 
	
    
}
