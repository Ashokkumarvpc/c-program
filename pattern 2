    1
   12
  123
 1234
12345


#include <stdio.h>
int main()
{
    int a;
    scanf("%d",&a);
    for(int i=a-1;i>=0;i--)
    {
        int n=1;
        for(int j=0;j<a;j++)
        {
             
            if(j>=i)
            {
                 printf("%d",n);
                 n++;}
            else
                printf(" ");
         
        }
        printf("\n");
    } 
	
    
}
