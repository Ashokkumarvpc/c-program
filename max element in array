#include <stdio.h>
#include<string.h>
int main() {
	int a,d;
	int b[10];
	
	scanf("%d",&d);
	scanf("%d",&a);

	for(int g=0;g<d;g++)
	{
	    int v=0;
	    for(int i=0;i<a;i++)
	    {
	        scanf("%d",&b[i]);
	    }
	    for(int j=0;j<a;j++)
	    {
	        if(b[j]>v)
	        v=b[j];
	    }
	printf("%d\n",v);
	}

}
