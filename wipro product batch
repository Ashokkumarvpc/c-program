#include <stdio.h>

int main() {
	char c[100];
	scanf("%[^\n]",c);
	int a=strlen(c);
	int b=1,e=1,r;
	for(int i=1;i<a;i++)
	{
	    
	    if(c[i]==' ')
	    {
	        e++;
	        b=i;
	        printf("%d",b);
	        r=(b+1)/2;
	        for(int j=r-1;j<b;j++)
	        {••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••
	            printf("%c",c[j]);
	            
	        }
	         for(int k=0;k<r;k++)
	         {
	            printf("%c",c[k]);
	             
	         }     
	    
	    }
	}
	    
	
	return 0;
}••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••••
