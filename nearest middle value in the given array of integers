#include <stdio.h>
int main() {
    int a,y;
    float r,e,q=0,w=99999,t=9999;
    scanf("%d",&a);
        int b[a];
    for(int i=0;i<a;i++){
        scanf("%d",&b[i]);
        if(q<b[i])
            q=b[i];
        if(b[i]<w)
            w=b[i];}
    e=(q+w)/2;
    for(int i=0;i<a;i++){
        r=e-b[i];
        if(r<0)
            r=r*(-1);
       if(r<t){
           t=r; 
           y=i;}}
    printf("%d",b[y]);
    return 0;
}
